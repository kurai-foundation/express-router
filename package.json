{
  "name": "@kurai-io/express-router",
  "version": "0.0.0",
  "description": "Layer-2 framework based on express, which makes it faster and easier to build fault-tolerant backend applications",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "test": "jest --verbose --runInBand",
    "build": "vite build"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/express-router.mjs",
      "require": "./dist/express-router.js"
    },
    "./exceptions": {
      "types": "./dist/exceptions.d.ts",
      "import": "./dist/exceptions.mjs",
      "require": "./dist/exceptions.js"
    },
    "./types": {
      "types": "./dist/types.d.ts"
    },
    "./requests": {
      "types": "./dist/requests/index.d.ts"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts"
    }
  },
  "keywords": [
    "router",
    "express",
    "framework",
    "node",
    "js"
  ],
  "author": "Kurai Foundation",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.12.7",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.3",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kurai-foundation/express-router.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/kurai-foundation/express-router/issues"
  },
  "homepage": "https://github.com/kurai-foundation/express-router#readme",
  "dependencies": {
    "express": "^5.1.0",
    "joi": "^17.13.3"
  }
}
